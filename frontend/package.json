        "parser": "babel-eslint",
        "plugins": [
            "react"
        ],
        "settings": {
            "react": {
                "version": "999.999.999"
            }
        },
        "env": {
            "browser": true,
            "es6": true,
            "node": true
        },
        "extends": [
            "eslint:recommended",
            "plugin:react/recommended"
        ],
        "rules": {
            "no-console": [
                2,
                {
                    "allow": [
                        "warn",
                        "error",
                        "exception"
                    ]
                }
            ],
            "react/jsx-uses-react": 1,
            "react/react-in-jsx-scope": 1,
            "react/no-find-dom-node": 0
        }
    },
    "scripts": {
        "start": "endpoint=http://localhost:8080 node --max-old-space-size=4096 scripts/start",
        "test:debug": "DEBUG=1 ./scripts/test.sh",
        "test": "./scripts/test.sh",
        "check:style": "./scripts/lint.sh",
        "check:vulnerabilities": "./scripts/audit.sh",
        "check:outdated": "./scripts/checkOutdated.sh",
        "check:ts": "tsc --project ./ --noEmit",
        "check:scripts": "shellcheck **/*.sh",
        "check": "npm run check:scripts && npm run check:ts && npm run check:style -- --fix && (npm run check:vulnerabilities 2>/dev/null || :) ",
        "build": "build=true endpoint=http://localhost:8080 node --max-old-space-size=4096 scripts/deployXceder"
